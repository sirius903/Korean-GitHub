{
    "manifest_version" : 3,
    "name" : "Korean GitHub",
    "version" : "0.0.0",
    "description" : "Extensions for GitHub Korean Translation",
    "icons" : {
        "16" : "icon16.png", 
        "32" : "icon32.png", 
        "48" : "icon48.png", 
        "128" : "icon128.png"
    },
    "content_scripts" : [
        {
            "js" : [
                "index.js"
            ],
            "matches" : [
                "https://*.github.com/*"
            ]
        }
    ]
}